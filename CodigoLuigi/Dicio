ve_sensor_cor(min_esq_branco,max_esq_branco,min_dir_branco,max_dir_branco) #ta vendo BRANCO
ve_sensor_cor(min_esq_amarelo,max_esq_amarelo,min_dir_amarelo,max_dir_amarelo) #ta vendo AMARELO
ve_sensor_cor(min_esq_vermelho,max_esq_vermelho,min_dir_vermelho,max_dir_vermelho )#ta vendo VERMELHO
ve_sensor_cor(min_esq_azul, max_esq_azul, min_dir_azul, max_dir_azul)#ta vendo AZUL
ve_sensor_cor( min_esq_preto, max_esq_preto, min_dir_preto, max_dir_preto)#ta vendo PRETO
ve_sensor_cor(min_esq_borda,max_esq_borda,min_dir_borda,max_dir_borda)#ta vendo BORDA
ve_sensor_cor(min_esq_rampa, max_esq_rampa, min_dir_rampa, max_dir_rampa)#ta vendo RAMPA

alinha(min_esq_borda,max_esq_borda,min_dir_borda,max_dir_borda)#alinhou com a BORDA
alinha(min_esq_rampa, max_esq_rampa, min_dir_rampa, max_dir_rampa)#alinhou com a RAMPA
alinha(min_esq_preto, max_esq_preto, min_dir_preto, max_dir_preto)##alinhou com o PRETO frente


